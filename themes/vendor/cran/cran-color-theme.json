{
  "name": "CRAN",
  "author": "dieghernan",
  "semanticHighlighting": true,
  "type": "light",
  "tokenColors": [
    {
      "settings": {
        "background": "#F7F7F7",
        "foreground": "#333333"
      }
    },
    {
      "name": "",
      "scope": "comment",
      "settings": {
        "foreground": "#60A0B0",
        "fontStyle": "italic"
      }
    },
    {
      "name": "",
      "scope": "constant.language",
      "settings": {
        "foreground": "#880000"
      }
    },
    {
      "name": "",
      "scope": [
        "constant.numeric",
        "constant.numeric.float"
      ],
      "settings": {
        "foreground": "#40A070"
      }
    },
    {
      "name": "",
      "scope": [
        "entity.name",
        "entity.name.tag.css",
        "entity.other.attribute-name.class"
      ],
      "settings": {
        "foreground": "#06287E"
      }
    },
    {
      "name": "",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#7D9029"
      }
    },
    {
      "name": "",
      "scope": "invalid",
      "settings": {
        "foreground": "#FF0000",
        "fontStyle": "bold"
      }
    },
    {
      "name": "",
      "scope": [
        "keyword",
        "keyword.control"
      ],
      "settings": {
        "foreground": "#007020",
        "fontStyle": "bold"
      }
    },
    {
      "name": "",
      "scope": "keyword.control.import",
      "settings": {
        "foreground": "#008000",
        "fontStyle": "bold"
      }
    },
    {
      "name": "",
      "scope": [
        "keyword.operator",
        "string"
      ],
      "settings": {
        "foreground": "#4070A0"
      }
    },
    {
      "name": "",
      "scope": [
        "entity.name.section.markdown",
        "markup.heading"
      ],
      "settings": {
        "foreground": "#BA2121",
        "fontStyle": "italic"
      }
    },
    {
      "name": "",
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "",
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "",
      "scope": [
        "markup.inline.raw",
        "markup.raw"
      ],
      "settings": {
        "foreground": "#007020"
      }
    },
    {
      "name": "",
      "scope": "storage.type",
      "settings": {
        "foreground": "#902000"
      }
    },
    {
      "name": "",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#BB6688"
      }
    },
    {
      "name": "",
      "scope": "support",
      "settings": {
        "foreground": "#008000"
      }
    },
    {
      "name": "",
      "scope": "variable",
      "settings": {
        "foreground": "#19177C"
      }
    },
    {
      "name": "",
      "scope": [
        "entity.name.tag.doctype.html",
        "meta.doctype",
        "meta.tag.sgml.doctype"
      ],
      "settings": {
        "foreground": "#60A0B0"
      }
    }
  ],
